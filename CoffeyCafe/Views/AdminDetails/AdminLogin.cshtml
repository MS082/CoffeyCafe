
@{
    ViewBag.Title = "AdminLogin";
}


<!-- contact -->
<div class="contact">
    <div class="container">
        <h4 class="title wow fadeInDown animated" data-wow-delay=".5s">Admin Login </h4>
         <div class="contact-form">
            
            <div class="col-md-7 contact-right wow fadeInRight animated" data-wow-delay=".5s">
                
                
                <form method="post" action="AdminLoginCheck">
                    
                    <input class="email" name="AdminName" id="AdminName" type="text" placeholder="Email" required="">
                    <input type="text" name="AdminPassword" id="AdminPassword" placeholder="Password" required="">
                    <br/>
                    <input class="wow zoomIn animated" data-wow-delay=".5s" type="submit" value="SUBMIT">
                    <input class="wow zoomIn animated" data-wow-delay=".5s" type="reset" value="CLEAR">
                </form>
            </div>
            <div class="clearfix"></div>
        </div>
    </div>
</div>
<!-- //contact -->
